<file path="landing/style.css">
/* ===== RESET Y GLOBALES ===== */
* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

:root {
    --primary-color: #ED3729; /* Rojo salmón */
    --primary-dark: #C41E17;
    --secondary-color: #FFE8E5; /* Rojo salmón muy claro para fondos */
    --text-dark: #333;
    --text-light: #555;
    --bg-light: #f9f9f9;
    --white: #ffffff;
    --shadow: 0 4px 15px rgba(0, 0, 0, 0.05);
}

html {
    scroll-behavior: smooth;
}

body {
    font-family: 'Poppins', sans-serif;
    line-height: 1.6;
    color: var(--text-dark);
    background-color: var(--white);
}

.container {
    max-width: 1100px;
    margin: 0 auto;
    padding: 0 20px;
}

.section {
    padding: 80px 0;
}

.text-center {
    text-align: center;
}

h1, h2, h3 {
    line-height: 1.3;
    margin-bottom: 20px;
    font-weight: 600;
}

h1 { font-size: 2.8rem; }
h2 { font-size: 2.2rem; }
h3 { font-size: 1.5rem; }
p { margin-bottom: 15px; color: var(--text-light); }

/* ===== BOTONES ===== */
.btn {
    display: inline-block;
    padding: 12px 28px;
    border-radius: 50px;
    text-decoration: none;
    font-weight: 600;
    transition: all 0.3s ease;
    cursor: pointer;
}

.btn-primary {
    background-color: var(--primary-color);
    color: var(--white);
}

.btn-primary:hover {
    background-color: var(--primary-dark);
    transform: translateY(-2px);
}

.store-buttons {
    display: flex;
    flex-wrap: wrap;
    gap: 15px;
    margin-top: 30px;
}

.store-btn {
    display: flex;
    align-items: center;
    background-color: #111;
    color: var(--white);
    padding: 10px 20px;
    border-radius: 8px;
    text-decoration: none;
    transition: background-color 0.3s ease;
    cursor: pointer;
    border: none;
}

.store-btn:hover {
    background-color: #333;
}

.store-btn i {
    font-size: 2.5rem;
    margin-right: 15px;
}

.store-btn div {
    display: flex;
    flex-direction: column;
    text-align: left;
}

.store-btn span {
    font-size: 0.8rem;
    text-transform: uppercase;
    opacity: 0.8;
}

.store-btn strong {
    font-size: 1.1rem;
}


/* ===== HEADER ===== */
header {
    background: var(--white);
    box-shadow: var(--shadow);
    position: sticky;
    top: 0;
    z-index: 1000;
}

header nav {
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 70px;
}

header nav > div {
    display: flex;
    align-items: center;
    gap: 10px;
}

.logo-image {
    height: 40px;
    width: auto;
}

.logo {
    font-size: 1.8rem;
    font-weight: 700;
    color: var(--primary-color);
    text-decoration: none;
}

/* ===== HERO ===== */
#hero {
    background-color: var(--secondary-color);
    padding: 60px 0;
    min-height: 80vh;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    position: relative;
}

.hero-content {
    display: flex;
    align-items: center;
    gap: 40px;
}

.hero-text {
    flex: 1.2;
}

.hero-text h1 {
    color: var(--primary-dark);
}

.hero-scroll-btn {
    position: absolute;
    bottom: 30px;
    left: 50%;
    transform: translateX(-50%);
    width: 45px;
    height: 45px;
    display: flex;
    align-items: center;
    justify-content: center;
    background: var(--primary-color);
    color: var(--white);
    border-radius: 50%;
    font-size: 1.5rem;
    text-decoration: none;
    transition: all 0.3s ease;
    animation: bounce 2s infinite;
}

.hero-scroll-btn:hover {
    background: var(--primary-dark);
    transform: translateX(-50%) scale(1.1);
}

@keyframes bounce {
    0%, 100% {
        transform: translateX(-50%) translateY(0);
    }
    50% {
        transform: translateX(-50%) translateY(-10px);
    }
}

.hero-image {
    flex: 1;
    display: flex;
    justify-content: center;
    position: relative;
}

.hero-image::before {
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    bottom: 0;
    width: 100px;
    background: linear-gradient(to right, var(--secondary-color), transparent);
    pointer-events: none;
    z-index: 1;
}

.hero-image img {
    max-width: 640px;
    width: 100%;
    filter: drop-shadow(0 10px 20px rgba(0,0,0,0.1));
}

/* ===== PROBLEM (Carrusel) ===== */
#problem {
    background-color: var(--bg-light);
}

/* Contenedor principal del carrusel */
.carousel {
    margin-top: 30px;
    position: relative;
    padding: 10px 60px 30px 60px;
    overflow: hidden;
    width: 100%;
}

.carousel-btn {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    background: var(--primary-color);
    color: var(--white);
    border: none;
    width: 45px;
    height: 45px;
    border-radius: 50%;
    cursor: pointer;
    font-size: 1.2rem;
    display: flex;
    align-items: center;
    justify-content: center;
    transition: all 0.2s ease;
    z-index: 10;
}

.carousel-btn-prev {
    left: 10px;
}

.carousel-btn-next {
    right: 10px;
}

.carousel-btn:hover {
    background: var(--primary-dark);
    transform: translateY(-50%) scale(1.1);
}

.carousel-btn:active {
    transform: translateY(-50%) scale(0.95);
}

.carousel-track {
    display: flex;
    transition: transform 0.5s ease-in-out;
    will-change: transform;
}

.carousel-slide {
    min-width: 100%;
    box-sizing: border-box;
    padding: 30px 20px;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.carousel-slide i {
    font-size: 3rem;
    color: var(--primary-color);
    margin-bottom: 15px;
}

.carousel-slide h4 {
    margin-bottom: 10px;
    color: var(--text-dark);
}

.carousel-slide p {
    color: var(--text-light);
    max-width: 700px;
}

.carousel-dots {
    display: flex;
    justify-content: center;
    gap: 10px;
    margin-top: 18px;
}

.carousel-dot {
    width: 12px;
    height: 12px;
    background: rgba(0,0,0,0.12);
    border-radius: 50%;
    border: none;
    cursor: pointer;
    transition: background 0.2s ease, transform 0.2s ease;
}

.carousel-dot[aria-selected="true"], .carousel-dot.active {
    background: var(--primary-color);
    transform: scale(1.05);
}

@media (min-width: 900px) {
    .carousel-slide {
        padding: 50px 40px;
    }
}

/* ===== FEATURES ===== */
.feature-row {
    display: flex;
    align-items: center;
    gap: 50px;
    margin-top: 60px;
}

.feature-row.reverse {
    flex-direction: row-reverse;
}

.feature-image-wrapper {
    flex: 1;
    display: flex;
    justify-content: center;
}

.feature-image-wrapper img {
    max-width: 320px;
    width: 100%;
    border-radius: 20px;
    box-shadow: var(--shadow);
}

.feature-text {
    flex: 1;
}

/* ===== TESTIMONIALS ===== */
#testimonials {
    background-color: var(--secondary-color);
}

.testimonial-grid {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: 30px;
    margin-top: 50px;
}

.testimonial-card {
    background: var(--white);
    padding: 40px;
    border-radius: 15px;
    box-shadow: var(--shadow);
    transition: all 0.3s ease;
    border-top: 4px solid var(--primary-color);
    position: relative;
    overflow: hidden;
}

.testimonial-card:hover {
    transform: translateY(-8px);
    box-shadow: 0 12px 30px rgba(237, 55, 41, 0.15);
}

.testimonial-card::before {
    content: '"';
    position: absolute;
    top: 10px;
    right: 20px;
    font-size: 3rem;
    color: var(--primary-color);
    opacity: 0.2;
}

.testimonial-header {
    display: flex;
    align-items: center;
    gap: 15px;
    margin-bottom: 20px;
}

.testimonial-avatar {
    width: 50px;
    height: 50px;
    border-radius: 50%;
    object-fit: cover;
    border: 3px solid var(--primary-color);
}

.testimonial-info {
    flex: 1;
}

.author-name {
    font-weight: 700;
    color: var(--text-dark);
    margin: 0;
    font-size: 1rem;
}

.author-role {
    font-size: 0.85rem;
    color: var(--text-light);
    margin: 3px 0 0 0;
}

.stars {
    color: #f39c12;
    margin-bottom: 15px;
    font-size: 1.1rem;
    letter-spacing: 2px;
}

.quote {
    font-style: italic;
    font-size: 1rem;
    color: var(--text-dark);
    line-height: 1.6;
    margin: 0;
}

/* ===== FINAL CTA ===== */
#final-cta {
    background-color: var(--primary-color);
    color: var(--white);
}

#final-cta h2 {
    color: var(--white);
}

#final-cta p {
    color: rgba(255, 255, 255, 0.9);
    font-size: 1.2rem;
    max-width: 600px;
    margin-left: auto;
    margin-right: auto;
}

#final-cta .store-buttons {
    justify-content: center;
}

/* ===== FAQ ===== */
#faq {
    background-color: var(--white);
}

.faq-container {
    max-width: 800px;
    margin: 0 auto;
}

.faq-item {
    margin-bottom: 15px;
    border: 1px solid #e0e0e0;
    border-radius: 10px;
    overflow: hidden;
    transition: all 0.3s ease;
}

.faq-item:hover {
    border-color: var(--primary-color);
    box-shadow: 0 4px 15px rgba(237, 55, 41, 0.1);
}

.faq-header {
    width: 100%;
    padding: 20px;
    background: var(--white);
    border: none;
    cursor: pointer;
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 1.1rem;
    font-weight: 600;
    color: var(--text-dark);
    transition: all 0.3s ease;
}

.faq-header:hover {
    background: rgba(237, 55, 41, 0.05);
    color: var(--primary-color);
}

.faq-title {
    text-align: left;
}

.faq-icon {
    color: var(--primary-color);
    transition: transform 0.3s ease;
    font-size: 1rem;
}

.faq-item.active .faq-icon {
    transform: rotate(180deg);
}

.faq-content {
    max-height: 0;
    overflow: hidden;
    transition: max-height 0.3s ease;
    background: rgba(237, 55, 41, 0.02);
}

.faq-item.active .faq-content {
    max-height: 500px;
}

.faq-content p {
    padding: 20px;
    margin: 0;
    color: var(--text-light);
    line-height: 1.8;
}

/* ===== FOOTER ===== */
footer {
    background-color: #1c1c1c;
    color: #aaa;
    padding: 50px 0 30px 0;
}

.footer-content {
    text-align: center;
}

.footer-links {
    margin-bottom: 20px;
}

.footer-links a {
    color: #eee;
    margin: 0 15px;
    text-decoration: none;
    font-size: 0.9rem;
}

.footer-links a:hover {
    color: var(--primary-color);
}

.disclaimer {
    font-size: 0.8rem;
    font-style: italic;
    max-width: 800px;
    margin: 20px auto;
    color: #777;
}

.copyright {
    font-size: 0.9rem;
    margin-top: 20px;
    color: #888;
}

/* ===== ANIMACIONES (Scroll) ===== */
.section {
    opacity: 0;
    transform: translateY(30px);
    transition: opacity 0.6s ease-out, transform 0.6s ease-out;
}

.section.is-visible {
    opacity: 1;
    transform: translateY(0);
}

/* ===== RESPONSIVE DESIGN (Móviles) ===== */
@media (max-width: 768px) {
    h1 { font-size: 2.2rem; }
    h2 { font-size: 1.8rem; }

    /* Hero */
    .hero-content {
        flex-direction: column;
        text-align: center;
    }
    
    .hero-text {
        order: 2;
    }

    .hero-image {
        order: 1;
        margin-bottom: 30px;
    }

    .hero-image img {
        max-width: 280px;
    }

    .store-buttons {
        justify-content: center;
    }

    /* Problem */
    .problem-features {
        flex-direction: column;
        align-items: center;
    }

    /* Features */
    .feature-row {
        flex-direction: column !important; /* !important para sobreescribir .reverse */
        text-align: center;
    }

    .feature-image-wrapper {
        margin-bottom: 30px;
    }

    /* Testimonials */
    .testimonial-grid {
        grid-template-columns: 1fr;
    }

    .testimonial-card {
        padding: 30px;
    }
}

/* ===== MODAL ===== */
.modal {
    display: none;
    position: fixed;
    z-index: 2000;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.6);
    backdrop-filter: blur(4px);
    animation: fadeIn 0.3s ease;
}

.modal.active {
    display: flex;
    align-items: center;
    justify-content: center;
}

@keyframes fadeIn {
    from {
        opacity: 0;
    }
    to {
        opacity: 1;
    }
}

@keyframes slideIn {
    from {
        transform: translateY(-50px);
        opacity: 0;
    }
    to {
        transform: translateY(0);
        opacity: 1;
    }
}

.modal-content {
    background: var(--white);
    padding: 40px;
    border-radius: 15px;
    width: 90%;
    max-width: 500px;
    box-shadow: 0 10px 40px rgba(0, 0, 0, 0.2);
    position: relative;
    animation: slideIn 0.3s ease;
}

.modal-close {
    position: absolute;
    top: 15px;
    right: 15px;
    background: none;
    border: none;
    font-size: 1.5rem;
    color: var(--text-light);
    cursor: pointer;
    transition: all 0.2s ease;
    width: 40px;
    height: 40px;
    display: flex;
    align-items: center;
    justify-content: center;
    border-radius: 50%;
}

.modal-close:hover {
    background: rgba(237, 55, 41, 0.1);
    color: var(--primary-color);
}

.modal-header {
    text-align: center;
    margin-bottom: 30px;
}

.modal-icon {
    font-size: 3rem;
    color: var(--primary-color);
    margin-bottom: 15px;
    display: block;
}

.modal-header h2 {
    color: var(--text-dark);
    margin-bottom: 0;
}

.modal-description {
    text-align: center;
    color: var(--text-light);
    margin-bottom: 30px;
    line-height: 1.6;
}

.modal-form {
    display: flex;
    flex-direction: column;
    gap: 15px;
}

.form-group {
    position: relative;
}

.form-group input {
    width: 100%;
    padding: 12px 15px;
    border: 2px solid #e0e0e0;
    border-radius: 8px;
    font-size: 1rem;
    font-family: 'Poppins', sans-serif;
    transition: all 0.3s ease;
}

.form-group input:focus {
    outline: none;
    border-color: var(--primary-color);
    box-shadow: 0 0 0 3px rgba(237, 55, 41, 0.1);
}

.modal-btn {
    width: 100%;
    padding: 12px 20px;
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 10px;
    font-size: 1rem;
}

.form-notice {
    text-align: center;
    font-size: 0.85rem;
    color: var(--text-light);
    margin: 0;
}

@media (max-width: 768px) {
    .modal-content {
        width: 95%;
        padding: 30px 20px;
    }
}
